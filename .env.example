
# SWE-QA Annotation System Configuration

# API Configuration
# URL where the FastAPI backend is running - MUST include the full path including '/api'
VITE_API_URL=http://localhost:8000/api
# API key for authentication (must match the backend API_KEY)
VITE_API_KEY=your_api_key_here
# Set to 'true' during development to use mock data instead of API calls
VITE_USE_MOCK_DATA=false

# Database Configuration
# SQLite example: sqlite:///./swe_qa.db
# PostgreSQL example: postgresql://user:password@localhost/swe_qa
DATABASE_URL=sqlite:///./swe_qa.db

# Google Authentication
# Client ID from Google Cloud Console (https://console.cloud.google.com)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Feature Flags
# Enable experimental features (default: false)
VITE_ENABLE_EXPERIMENTAL=false
# Show detailed error messages (default: true for development)
VITE_DETAILED_ERRORS=true
# Enable real-time updates (default: false)
VITE_ENABLE_REALTIME=false
